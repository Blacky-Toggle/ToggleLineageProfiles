<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ColorPickerPreference">
        <attr name="showTransparencyBar" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ColorPickerBar">
        <attr name="component" format="enum">
            <enum name="hue" value="0" />
            <enum name="saturation" value="1" />
            <enum name="luminance" value="2" />
            <enum name="alpha" value="3" />
        </attr>
    </declare-styleable>

</resources>